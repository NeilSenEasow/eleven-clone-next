{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/server.js" },
    { "src": "/(.*)", "dest": "/dist/server.js" }
  ]
}
